{
	"name": "backend-pals-example",
	"version": "1.0.0",
	"description": "Example for a full backend application",
	"main": "server.js",
	"scripts": {
		"dev": "tsx watch --inspect src/server.ts",
		"build": "tsc -p .",
		"start": "node dist/server.js",
		"prisma:generate": "prisma generate --schema=src/shared/database/prisma/schema.prisma",
		"prisma:migrate": "prisma migrate dev --schema=src/shared/database/prisma/schema.prisma",
		"prisma:studio": "prisma studio --schema=src/shared/database/prisma/schema.prisma"
	},
	"keywords": [
		"backend",
		"example"
	],
	"author": "Gustavo Pals",
	"license": "ISC",
	"dependencies": {
		"@prisma/client": "^5.18.0",
		"cors": "^2.8.5",
		"express": "^4.19.2"
	},
	"devDependencies": {
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/node": "^22.3.0",
		"prisma": "^5.18.0",
		"tsx": "^4.17.0",
		"typescript": "^5.5.4"
	}
}
